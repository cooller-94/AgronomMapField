@model GoogleMaps.Models.FilterJobAccauntinData

@using (Ajax.BeginForm("GetJobForCalendar", "GoogleMap", new AjaxOptions { OnSuccess = "Statistics.OnSuccessFilter", HttpMethod = "POST" }, new { id = "filter-form" }))
{
    <fieldset class="filter-option">
        @Html.LabelFor(item => item.Year)
        @Html.DropDownListFor(d => d.Year, (List<SelectListItem>)ViewBag.Years)
    </fieldset>

    <fieldset class="filter-option">
        @Html.LabelFor(item => item.FieldId)
        @Html.DropDownListFor(d => d.FieldId, (List<SelectListItem>)ViewBag.Fields)
    </fieldset>

    <input type="submit" value="Применить" />
}

